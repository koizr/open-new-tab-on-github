{
  "manifest_version": 2,
  "name": "Open new tab",
  "description": "open links on GitHub with new tab",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["app.js"]
    }
  ],
  "options_page": "options.html",
  "permissions": ["https://github.com/*", "storage"]
}
